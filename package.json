{
  "name": "sheetsy",
  "version": "1.0.0",
  "description": "Use your Google Sheets as a data source in the browser.  Like tabletop.js but simpler.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/test-with-fixtures.js",
    "test:node": "node test/test-without-fixtures.js",
    "test:browser": "browserify test/test-without-fixtures.js | tape-run",
    "test:all": "npm run test && npm run test:node && npm run test:browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/sheetsy.git"
  },
  "keywords": [
    "google",
    "sheets",
    "spreadsheet",
    "sheet",
    "tabletop",
    "browser"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/sheetsy/issues"
  },
  "homepage": "https://github.com/TehShrike/sheetsy#readme",
  "devDependencies": {
    "browser-run": "3.5.0",
    "browserify": "14.4.0",
    "tape": "4.7.0",
    "tape-run": "2.4.1"
  },
  "dependencies": {
    "got": "7.1.0",
    "ordered-entries": "1.0.0"
  },
  "browser": {
    "./get.js": "./browser-get.js"
  }
}
